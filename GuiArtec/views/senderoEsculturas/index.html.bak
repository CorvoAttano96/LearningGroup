          function(data){
            var len = data.length;
            var bounds = [];
            var path = [];
            for(i=0; i< len ; i++){
                var lat = data[i].lat;
                var lng = data[i].lng;
                var title = data[i].title;
                //var info = data[i].infoWindow;
