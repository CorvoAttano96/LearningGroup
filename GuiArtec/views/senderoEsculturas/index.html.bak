                          lng: tulng,
                          radius: 150,
                          strokeColor: 'transparent',
                          strokeOpacity: 1,
                          strokeWeight: 3,
                          fillColor: 'transparent'
                        });

                        //Oculto los marcadores fuera del rango 2
